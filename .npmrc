# NPM Configuration for nuxt-mixpanel

# Registry configuration
registry=https://registry.npmjs.org/

# Package-lock configuration
package-lock=true
package-lock-only=false

# Save configuration
save=true
save-exact=false
save-prefix=^

# Security settings
audit-level=moderate
fund=false

# Publishing configuration
access=public

# Engine strict mode
engine-strict=true

# Optional dependencies
optional=true

# Progress and logging
progress=true
loglevel=warn

# Cache settings
cache-max=2592000000
cache-min=86400
